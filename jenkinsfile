pipeline {
    agent any

    stages {
        stage ('Compile stage') {

            steps {
                withMaven(maven : 'maven_3_5_0') {
                    sh 'mvn clean compile'
                }
            }
        }
        
        stage ('Test stage') {

            steps {
                withMaven(maven : 'maven_3_5_0') {
                    sh 'mvn clean Testing'
                }
            }
        }

        stage ('Deploy stage') {

            steps {
                withMaven(maven : 'maven_3_5_0') {
                    sh 'mvn clean Deploy'
                }
            }
        }


    }
}